        [ "+", "±" ],
        [ "=", "≠" ],
        [ "<", "≤" ],
        [ ">", "≥" ],
        [ "~", "≈" ],
        [ "%", "‰" ],
        [ "|", "¦" ],
        [ "a", "ä" ],
        [ "A", "Ä" ],
        [ "c", "ĉ" ],
        [ "C", "Ĉ" ],
        [ "e", "é" ],
        [ "E", "É" ],
        [ "g", "ĝ" ],
        [ "G", "Ĝ" ],
        [ "h", "ĥ" ],
        [ "H", "Ĥ" ],
        [ "i", "í" ],
        [ "I", "Í" ],
        [ "j", "ĵ" ],
        [ "J", "Ĵ" ],
        [ "s", "ŝ" ],
        [ "S", "Ŝ" ],
        [ "u", "ŭ" ],
        [ "U", "Ŭ" ],
        [ "[", "å" ],
        [ "{", "Å" ],
        [ "'", "ä" ],
        [ "\"", "Ä" ],
        [ "o", "ö" ],
        [ "O", "Ö" ],
