    { "pos": "O", "letters": [ "y", "Y" ] },
    { "pos": "P", "letters": [ ";", ":" ] },
    { "pos": "[", "letters": [ "[", "{" ] },
    { "pos": "]", "letters": [ "]", "}" ] },
    { "pos": "\\", "letters": [ "\\", "|" ] },
    { "pos": "A", "letters": [ "a", "A" ] },
    { "pos": "S", "letters": [ "r", "R" ] },
    { "pos": "D", "letters": [ "s", "S" ] },
    { "pos": "F", "letters": [ "t", "T" ] },
    { "pos": "G", "letters": [ "g", "G" ] },
    { "pos": "H", "letters": [ "k", "K" ] },
    { "pos": "J", "letters": [ "n", "N" ] },
    { "pos": "K", "letters": [ "e", "E" ] },
    { "pos": "L", "letters": [ "i", "I" ] },
    { "pos": ";", "letters": [ "o", "O" ] },
    { "pos": "'", "letters": [ "'", "\"" ] },
    { "pos": "Iso", "letters": [ "cdk:special", "" ] },
    { "pos": "Z", "letters": [ "z", "Z" ] },
    { "pos": "X", "letters": [ "x", "X" ] },
    { "pos": "C", "letters": [ "c", "C" ] },
    { "pos": "V", "letters": [ "d", "D" ] },
    { "pos": "B", "letters": [ "v", "V" ] },
    { "pos": "N", "letters": [ "m", "M" ] },
    { "pos": "M", "letters": [ "h", "H" ] },
    { "pos": ",", "letters": [ ",", "<" ] },
    { "pos": ".", "letters": [ ".", ">" ] },
    { "pos": "/", "letters": [ "/", "?" ] }
  ],
  "customDeadKeys": [
    {
      "name": "special",
      "baseChar": "+",
      "stringMap": [
        [ "r", "®" ],
        [ "t", "™" ],
        [ "d", "°" ],
